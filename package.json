{
  "name": "scalable-socket",
  "version": "0.1.0",
  "description": "Scalable websockets with Nest and Redis.",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "lint": "eslint 'src/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.13",
    "@nestjs/core": "^7.0.13",
    "@nestjs/platform-express": "^7.0.13",
    "@nestjs/platform-socket.io": "^7.0.13",
    "@nestjs/websockets": "^7.0.13",
    "dotenv": "^8.2.0",
    "faker": "^4.1.0",
    "hbs": "^4.1.1",
    "helmet": "^3.22.0",
    "ioredis": "^4.17.1",
    "reflect-metadata": "^0.1.13",
    "rethinkdb": "^2.4.2",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "socket.io-redis": "^5.2.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.5",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/helmet": "^0.0.47",
    "@types/ioredis": "^4.16.2",
    "@types/jest": "^25.2.2",
    "@types/node": "^14.0.1",
    "@types/socket.io": "^2.1.5",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.13.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.2",
    "yarn-upgrade-all": "^0.5.2"
  }
